frame.bottom=2
frame.left=2
frame.right=2
text.focus.color=white
text.bold=true

[ProgressbarContents]
inherits=PanelButtonTool
frame=true
frame.element=progress-pattern
interior=false
frame.top=2
frame.bottom=2
frame.left=2
frame.right=2

[ItemView]
inherits=PanelButtonTool
text.margin=false
frame=false
interior.element=itemview

[Splitter]
inherits=PanelButtonTool
interior.element=splitter
frame.element=splitter
frame.top=1
frame.bottom=1
frame.left=1
frame.right=1
indicator.element=splitter-grip
indicator.size=16

[Menu]
inherits=PanelButtonTool
frame.element=menu
interior.element=menu
frame.top=4
frame.bottom=4
frame.left=8
frame.right=8

[MenuItem]
inherits=PanelButtonTool
frame=true
interior.element=menuitem
frame.element=menuitem
indicator.element=menuitem
min_height=22
frame.top=1
frame.bottom=1
frame.left=8
frame.right=8

